{
  "name": "docuframe",
  "private": true,
  "version": "0.1.0",
  "type": "commonjs",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "vite",
    "setup": "node setup.js",
    "validate-setup": "node setup.js --validate",
    "start-backend": "cd server && npm run dev",
    "install-backend": "cd server && npm install",
    "dev-all": "concurrently \"npm run start-backend\" \"npm start\"",
    "setup-all": "npm run setup && npm run install-backend && echo \"Setup complete! Run: npm run dev-all\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "adm-zip": "^0.5.16",
    "framer-motion": "^11.0.5",
    "lucide-react": "^0.513.0",
    "pdf-lib": "^1.17.1",
    "pdf2json": "^3.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^7.6.0",
    "electron": "^29.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}
